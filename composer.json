{
    "name": "vkbansal/frontmatter",
    "description": "Frontmatter allows page-specific variables to be included at the top of a template using the YAML,JSON or INI format.",
    "keywords": ["yaml", "front-matter", "parser", "frontmatter", "json", "front", "matter", "ini", "metadata"],
    "homepage": "https://github.com/vkbansal/FrontMatter",
    "license": "MIT",
    "authors": [
        {
            "name": "Vivek Kumar Bansal",
            "email": "contact@vkbansal.me",
            "homepage": "http://vkbansal.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "symfony/yaml": "2.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*"
    },
    "autoload": {
        "psr-4" : {
            "VKBansal\\FrontMatter\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
